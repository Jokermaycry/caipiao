<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/bg_white" />
        </shape>
    </item>
    <item>

        <shape android:shape="oval">
            <size
                android:width="20dp"
                android:height="20dp" />
            <stroke
                android:width="3dp"
                android:color="@color/line_black" />
        </shape>
    </item>

    <item
        android:bottom="4dp"
        android:left="4dp"
        android:right="4dp"
        android:top="4dp">
        <shape android:shape="oval">
            <solid android:color="@color/palette_black" />
        </shape>
    </item>

</layer-list>
